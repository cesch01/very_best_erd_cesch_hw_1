<div class="page-header">
  <h3>Venue Address #<%= @venue_address.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Address</dt>
      <dd><%= @venue_address.address %></dd>

      <dt>Neighborhood</dt>
      <dd><%= @venue_address.neighborhood %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/venue_addresses" class="btn btn-primary">
        Back
      </a>
      <a href="/venue_addresses/<%= @venue_address.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_venue_address/<%= @venue_address.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
