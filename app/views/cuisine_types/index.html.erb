<div class="page-header">
  <h1>
    Cuisine types
    <a href="/cuisine_types/new" class="btn btn-lg btn-success">New Cuisine type</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Actions</th>
      </tr>

      <% @cuisine_types.each do |cuisine_type| %>
      <tr>
        <td><%= cuisine_type.name %></td>
        <td>
          <a href="/cuisine_types/<%= cuisine_type.id %>" class="btn btn-primary">Show</a>
          <a href="/cuisine_types/<%= cuisine_type.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_cuisine_type/<%= cuisine_type.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @cuisine_types, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
